                <stringProp name="filename"></stringProp>
                <stringProp name="script">import org.json.*;
String message = prev.getResponseDataAsString().trim();

bsh.shared.util.info(&quot;Ide server(syncPlaygroundInfo)原始 WSS 消息: &quot; + message);

if (message.length() &lt; 3) {
    return;  // 太短，直接跳过
}

String prefix = message.substring(0, 2);
String payload = message.substring(2);

try {
    if (&quot;42&quot;.equals(prefix)) {
        // 标准 socket.io event 消息
        JSONArray array = new JSONArray(payload);
        if (array.length() &gt;= 2) {
            String eventName = array.getString(0);
            
            bsh.shared.util.info(&quot;Ide server(syncPlaygroundInfo)事件类型: &quot; + eventName);
            
            if (&quot;syncPlaygroundInfo&quot;.equals(eventName)) {
                JSONObject data = array.getJSONObject(1);
                String dockerId = data.optString(&quot;dockerId&quot;, &quot;&quot;);
                
                if (!dockerId.isEmpty()) {
                    bsh.shared.util.info(&quot;Ide server(syncPlaygroundInfo) 成功获取 dockerId: &quot; + dockerId);
                    vars.put(&quot;dockerId&quot;, dockerId);
                } else {
                    bsh.shared.util.warn(&quot;Ide server(syncPlaygroundInfo) syncPlaygroundInfo 中缺少 dockerId 字段&quot;);
                }
            }
            // 可以在这里继续处理其他你关心的事件
            else if (&quot;resourceMonitoring&quot;.equals(eventName)) {
                JSONObject data = array.getJSONObject(1);
                // ... 记录内存、CPU 等
            }
        }
    }
    else if (&quot;40&quot;.equals(prefix)) {
        bsh.shared.util.info(&quot;Ide server(syncPlaygroundInfo) 收到 connect 包 (sid)&quot;);
    }
    else if (&quot;2&quot;.equals(prefix) || &quot;3&quot;.equals(prefix)) {
        bsh.shared.util.info(&quot;Ide server(syncPlaygroundInfo) 未知前缀: &quot; + prefix);
    }
    else {
        bsh.shared.util.info(&quot;Ide server(syncPlaygroundInfo) 未知前缀: &quot; + prefix);
    }
    
} catch (JSONException e) {
    bsh.shared.util.error(&quot;Ide server(syncPlaygroundInfo) JSON 解析失败: &quot; + e.getMessage() + &quot; | 原始消息: &quot; + message, e);
} catch (Exception e) {
    bsh.shared.util.error(&quot;Ide server(syncPlaygroundInfo) 非 JSON 异常: &quot; + e.getMessage(), e);
}

// 可以在这里添加其他逻辑，例如根据 dockerId 的值执行不同的操作</stringProp>
