                <stringProp name="script">import org.json.JSONArray;
import org.json.JSONObject;

try {
    String responseData = prev.getResponseDataAsString();

    // 1. 处理空响应（心跳/断连判断）
    if (responseData == null || responseData.trim().isEmpty()) {
        int pingCount = vars.get(&quot;pingCount&quot;) != null 
            ? Integer.parseInt(vars.get(&quot;pingCount&quot;)) : 0;
        pingCount++;
        vars.put(&quot;pingCount&quot;, String.valueOf(pingCount));

        int maxPing = vars.get(&quot;pingCountMax&quot;) != null 
            ? Integer.parseInt(vars.get(&quot;pingCountMax&quot;)) : 5;

        if (pingCount &gt;= maxPing) {
            vars.put(&quot;connectionBroken&quot;, &quot;true&quot;);
            log.warn(&quot;(Ai Agent)-session ⚠️ 连续 {} 次空响应，已达上限，标记断连&quot;, pingCount);
        } else {
            log.warn(&quot;(Ai Agent)-session ⚠️ 响应为空，pingCount = {} / {}&quot;, pingCount, maxPing);
        }
        return;
    }

    // 2. 日志预览（防止日志过长）
    String preview = responseData.length() &lt;= 400 
        ? responseData 
        : responseData.substring(0, 380) + &quot;...&quot;;
    bsh.shared.util.info(&quot;(Ai Agent)-session 收到响应: {}&quot;, preview);

    // 3. 定位 JSON 数组起始位置并分离状态码
    int bracketIndex = responseData.indexOf(&quot;[&quot;);
    if (bracketIndex &lt;= 0) {
        bsh.shared.util.error(&quot;(Ai Agent)-session 未找到有效的 [ 开头，前100字符: {}&quot;, 
            responseData.substring(0, Math.min(100, responseData.length())));
        return;
    }

    String statusCode = responseData.substring(0, bracketIndex).trim();
    String jsonPart    = responseData.substring(bracketIndex);

    bsh.shared.util.info(&quot;(Ai Agent)-session 帧类型: {}&quot;, statusCode);

    // 4. 只处理 4x 开头的帧（包含会话创建/角色相关消息）
    if (!statusCode.startsWith(&quot;4&quot;)) {
        return;
    }

    // 5. 解析 JSON
    JSONArray arr;
    try {
        arr = new JSONArray(jsonPart);
    } catch (Exception e) {
        bsh.shared.util.error(&quot;(Ai Agent)-session JSON 解析失败: {}&quot;, e.getMessage());
        return;
    }

    if (arr.length() == 0) {
        log.warn(&quot;(Ai Agent)-session 43x 帧为空数组&quot;);
        return;
    }

    JSONObject payload = arr.optJSONObject(0);
    if (payload == null) {
        log.warn(&quot;(Ai Agent)-session 43x 帧第一个元素不是 JSONObject&quot;);
        return;
    }

    String status = payload.optString(&quot;status&quot;, &quot;&quot;).trim().toLowerCase();
    if (!&quot;ok&quot;.equals(status)) {
        log.warn(&quot;(Ai Agent)-session 43x 帧 status 不是 ok，实际: {}&quot;, status);
        return;
    }

    JSONArray dataArray = payload.optJSONArray(&quot;data&quot;);
    if (dataArray == null || dataArray.length() == 0) {
        log.warn(&quot;(Ai Agent)-session 43x 帧缺少 data 数组或为空&quot;);
        return;
    }

    // 6. 遍历 data 数组，寻找 role_name = &quot;ChatRole&quot; 的记录
    for (int i = 0; i &lt; dataArray.length(); i++) {
        JSONObject item = dataArray.optJSONObject(i);
        if (item == null) continue;

        String roleName = item.optString(&quot;role_name&quot;, &quot;&quot;).trim();
        if (&quot;ChatRole&quot;.equals(roleName)) {
            String id = item.optString(&quot;id&quot;, &quot;&quot;).trim();
            if (!id.isEmpty()) {
                vars.put(&quot;session_id&quot;, id);
                bsh.shared.util.info(&quot;(Ai Agent)-session ✅ 找到 ChatRole，提取并保存 session_id: {}&quot;, id);

                // 成功获取后可选择立即结束监听循环
                vars.put(&quot;toolCallEndSessionId&quot;, &quot;true&quot;);
                bsh.shared.util.info(&quot;(Ai Agent)-session → 设置 toolCallEndSessionId = true，准备退出监听&quot;);
                return;  // 提前结束本次处理
            }
        }
    }

    // 如果遍历完都没找到符合条件的 ChatRole
    bsh.shared.util.warn(&quot;(Ai Agent)-session 未在 data 数组中找到 role_name=ChatRole 的记录&quot;);

} catch (Exception e) {
    bsh.shared.util.error(&quot;(Ai Agent) 整体处理异常: {}&quot;, e.getMessage(), e);
