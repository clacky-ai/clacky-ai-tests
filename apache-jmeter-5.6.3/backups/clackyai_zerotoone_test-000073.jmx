                <stringProp name="parameters"></stringProp>
                <stringProp name="filename"></stringProp>
                <stringProp name="script">import org.json.JSONArray;
import org.json.JSONObject;

try {
    String responseData = prev.getResponseDataAsString();

    // ── 1. 处理空响应（心跳/断连判断） ──
    if (responseData == null || responseData.trim().isEmpty()) {
        int pingCountLite = vars.get(&quot;pingCountLite&quot;) != null 
            ? Integer.parseInt(vars.get(&quot;pingCountLite&quot;)) 
            : 0;
        
        pingCountLite++;
        vars.put(&quot;pingCountLite&quot;, String.valueOf(pingCountLite));

        int pingCountMax = vars.get(&quot;pingCountMax&quot;) != null 
            ? Integer.parseInt(vars.get(&quot;pingCountMax&quot;)) 
            : 5;

        if (pingCountLite &gt;= pingCountMax) {
            vars.put(&quot;connectionBroken&quot;, &quot;true&quot;);
            log.warn(&quot;(Ai Agent) ⚠️ 连续 &quot; + pingCount + &quot; 次空响应，已达上限，标记断连&quot;);
        } else {
            log.warn(&quot;(Ai Agent) ⚠️ 响应为空，pingCountLite = &quot; + pingCountLite + &quot; / &quot; + pingCountMax);
        }
        return;
    }

    // ── 2. 日志预览（控制长度） ──
    String preview = responseData.length() &lt;= 400 
        ? responseData 
        : responseData.substring(0, 380) + &quot;...&quot;;
    bsh.shared.util.info(&quot;(Ai Agent) 收到响应: &quot; + preview);

    // ── 3. 提取帧类型和 JSON 部分 ──
    int bracketIndex = responseData.indexOf(&quot;[&quot;);
    if (bracketIndex &lt;= 0) {
        bsh.shared.util.error(&quot;(Ai Agent) 未找到有效的 [ 开头，前100字符: &quot; 
            + responseData.substring(0, Math.min(100, responseData.length())));
        return;
    }

    String statusCode = responseData.substring(0, bracketIndex).trim();
    String jsonPart    = responseData.substring(bracketIndex);

    bsh.shared.util.info(&quot;(Ai Agent) 帧类型: &quot; + statusCode);

    // ── 4. 只处理 4 开头的帧 ──
    if (statusCode.startsWith(&quot;4&quot;)) {
        try {
            JSONArray arr = new JSONArray(jsonPart);
            if (arr.length() == 0) {
                log.warn(&quot;(Ai Agent) &quot; + statusCode + &quot; 帧为空数组&quot;);
                return;
            }

            if (!(arr.get(0) instanceof JSONObject)) {
                log.warn(&quot;(Ai Agent) &quot; + statusCode + &quot; 帧第一个元素不是 JSONObject&quot;);
                return;
            }

            JSONObject payload = arr.getJSONObject(0);

            String status = payload.optString(&quot;status&quot;, &quot;&quot;).trim().toLowerCase();
            if (!&quot;ok&quot;.equals(status)) {
                log.warn(&quot;(Ai Agent) &quot; + statusCode + &quot; 帧 status 不是 ok，实际: &quot; + status);
                return;
            }

            JSONObject data = payload.optJSONObject(&quot;data&quot;);
            if (data == null) {
                log.warn(&quot;(Ai Agent) &quot; + statusCode + &quot; 帧缺少 data 字段&quot;);
                return;
            }

            // ── 核心判断：只看是否有 model_tier 且值为 premium ──
            String modelTier = data.optString(&quot;model_tier&quot;, null);

            if (modelTier == null) {
                bsh.shared.util.info(&quot;(Ai Agent) 该帧无 model_tier 字段，继续等待...&quot;);
                return;
            }

            modelTier = modelTier.trim().toLowerCase();

            if (&quot;premium&quot;.equals(modelTier)) {
                bsh.shared.util.info(&quot;✅ 检测到 model_tier = premium，视为初始化完成&quot;);

                // 可选：仍然记录下来供后续使用（但不强制）
                vars.put(&quot;model_tier&quot;, &quot;premium&quot;);

                // 结束 WebSocket 监听循环
                vars.put(&quot;toolCallEndLite&quot;, &quot;true&quot;);
                bsh.shared.util.info(&quot;→ 设置 toolCallEndLite = true，准备退出 WebSocket 监听&quot;);

                return;  // 提前结束本次处理
            } else {
                bsh.shared.util.warn(&quot;(Ai Agent) 检测到 model_tier = &quot; + modelTier + &quot; （非 premium），继续等待...&quot;);
                // 可选：可以在这里记录非 premium 的情况
                // vars.put(&quot;model_tier&quot;, modelTier);
                return;
            }

        } catch (Exception e) {
            bsh.shared.util.error(&quot;(Ai Agent) 解析 &quot; + statusCode + &quot; 帧失败: &quot; + e.getMessage(), e);
        }
    } 
    // 非 4 开头帧 → 安静忽略

} catch (Exception e) {
    bsh.shared.util.error(&quot;(Ai Agent) 整体处理异常: &quot; + e.getMessage(), e);
}</stringProp>
