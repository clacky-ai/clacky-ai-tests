                <stringProp name="script">import org.json.JSONArray;
import org.json.JSONObject;

try {
    String responseData = prev.getResponseDataAsString();

    // ── 1. 处理空响应（心跳/断连判断） ──
    if (responseData == null || responseData.trim().isEmpty()) {
        int pingCountInit = 0;
        try {
            pingCountInit = Integer.parseInt(vars.get(&quot;pingCountInit&quot;));
        } catch (Exception ignored) {}
        
        pingCountInit++;
        vars.put(&quot;pingCountInit&quot;, String.valueOf(pingCountInit));

        int pingCountMax = 5;
        try {
            pingCountMax = Integer.parseInt(vars.get(&quot;pingCountMax&quot;));
        } catch (Exception ignored) {}

        if (pingCountInit &gt;= pingCountMax) {
            vars.put(&quot;connectionBroken&quot;, &quot;true&quot;);
            log.warn(&quot;(Ai Agent)-session ⚠️ 连续 &quot; + pingCount + &quot; 次空响应，已达上限，标记断连&quot;);
        } else {
            log.warn(&quot;(Ai Agent)-session ⚠️ 响应为空，pingCountInit = &quot; + pingCountInit + &quot; / &quot; + pingCountMax);
        }
        return;
    }

    // ── 2. 日志预览（控制长度，避免日志爆炸） ──
    String preview = responseData.length() &lt;= 400 
        ? responseData 
        : responseData.substring(0, 380) + &quot;...&quot;;
    bsh.shared.util.info(&quot;(Ai Agent)-session 收到响应: &quot; + preview);

    // ── 3. 查找 JSON 数组起始位置 ──
    int bracketIndex = responseData.indexOf(&quot;[&quot;);
    if (bracketIndex &lt;= 0) {
        bsh.shared.util.error(&quot;(Ai Agent)-session 未找到有效的 [ 开头，数据前100字符: &quot; 
            + responseData.substring(0, Math.min(100, responseData.length())));
        return;
    }

    String statusCode = responseData.substring(0, bracketIndex).trim();
    String jsonPart    = responseData.substring(bracketIndex);

    bsh.shared.util.info(&quot;(Ai Agent)-session 帧类型: &quot; + statusCode);

    // ── 4. 只处理 43x 开头的会话创建消息 ──
    if (statusCode.startsWith(&quot;43&quot;)) {
        try {
            JSONArray arr = new JSONArray(jsonPart);
            if (arr.length() == 0) {
                log.warn(&quot;(Ai Agent)-session 43x 帧为空数组&quot;);
                return;
            }

            if (!(arr.get(0) instanceof JSONObject)) {
                log.warn(&quot;(Ai Agent)-session 43x 帧第一个元素不是 JSONObject&quot;);
                return;
            }

            JSONObject payload = arr.getJSONObject(0);

            String status = payload.optString(&quot;status&quot;, &quot;&quot;).trim().toLowerCase();
            if (!&quot;ok&quot;.equals(status)) {
                log.warn(&quot;(Ai Agent)-session 43x 帧 status 不是 ok，实际: &quot; + status);
                return;
            }

            JSONObject data = payload.optJSONObject(&quot;data&quot;);
            if (data == null) {
                log.warn(&quot;(Ai Agent)-session 43x 帧缺少 data 字段&quot;);
                return;
            }

            // ── 核心：提取 session_id ──
            String sessionId = data.optString(&quot;session_id&quot;, null);
            if (sessionId != null &amp;&amp; !sessionId.trim().isEmpty()) {
                String cleanSessionId = sessionId.trim();
                vars.put(&quot;session_id&quot;, cleanSessionId);
                bsh.shared.util.info(&quot;(Ai Agent)-session 43x ✅ 成功提取并保存 session_id: &quot; + cleanSessionId);

                // 获取到 session_id 后立即结束当前 WebSocket 链接的循环
                vars.put(&quot;toolCallEndSessionId&quot;, &quot;true&quot;);
                bsh.shared.util.info(&quot;(Ai Agent)-session 43x → 设置 toolCallEndSessionId = true，准备退出 WebSocket 监听&quot;);
