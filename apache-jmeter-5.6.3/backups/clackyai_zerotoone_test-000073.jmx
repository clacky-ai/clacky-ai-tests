    String responseData = prev.getResponseDataAsString();

    if (responseData == null || responseData.trim().isEmpty()) {
        int pingCount = Integer.parseInt(vars.get(&quot;pingCount&quot;));
        pingCount++;  // 响应为空时递增计数器
        vars.put(&quot;pingCount&quot;, String.valueOf(pingCount));
        int pingCountMax = Integer.parseInt(vars.get(&quot;pingCountMax&quot;));
        if (pingCount &gt;= pingCountMax) {
            // 达到最大值时标记断连或其他处理
            vars.put(&quot;connectionBroken&quot;, &quot;true&quot;);
            log.warn(&quot;(Ai Agent) ⚠️ 响应数据为空，已达到最大 pingCount: &quot; + pingCount + &quot;，标记断连&quot;);
        } else {
            log.warn(&quot;(Ai Agent) ⚠️ 响应数据为空，pingCount 递增为: &quot; + pingCount);
        }
        return;  // 提前退出
    } else {
        // 只在调试时打印完整响应，生产环境可注释或限制长度
        if (responseData.length() &lt; 2000) {
//            bsh.shared.util.info(&quot;(Ai Agent) 原始响应: &quot; + responseData);
        } else {
//            bsh.shared.util.info(&quot;(Ai Agent) 原始响应长度: &quot; + responseData.length() + &quot; (过长，未打印内容)&quot;);
        }

        int bracketIndex = responseData.indexOf(&quot;[&quot;);
        if (bracketIndex == -1) {
//            bsh.shared.util.error(&quot;(Ai Agent) ❌ 未找到 JSON 数组起始符号 [，responseData: &quot; + responseData.substring(0, Math.min(100, responseData.length())));
            return;
        }

        String statusCode = responseData.substring(0, bracketIndex).trim();
        String jsonArrayStr = responseData.substring(bracketIndex);

//        bsh.shared.util.info(&quot;(Ai Agent) 状态码: &quot; + statusCode);

        if (&quot;42&quot;.equals(statusCode)) {
            try {
                JSONArray jsonArray = new JSONArray(jsonArrayStr);

                // 检查第一个元素是否是 &quot;newMessage&quot; 且数组长度足够
                if (jsonArray.length() &gt;= 2 &amp;&amp; &quot;newMessage&quot;.equals(jsonArray.optString(0))) {
                    if (jsonArray.get(1) instanceof JSONObject) {
                        JSONObject messageObj = jsonArray.getJSONObject(1);

                        if (messageObj.has(&quot;content&quot;) &amp;&amp; messageObj.get(&quot;content&quot;) instanceof JSONObject) {
                            JSONObject content = messageObj.getJSONObject(&quot;content&quot;);

                            if (&quot;updateAgentState&quot;.equals(content.optString(&quot;event&quot;))) {
                                JSONObject data = content.optJSONObject(&quot;data&quot;);
                                if (data != null) {
                                    boolean isRunning = data.optBoolean(&quot;is_running&quot;, true);
//                                    bsh.shared.util.info(&quot;🔍 检测到 is_running = &quot; + isRunning);

