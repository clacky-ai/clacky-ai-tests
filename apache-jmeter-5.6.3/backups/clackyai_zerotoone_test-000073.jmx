            <BeanShellPostProcessor guiclass="TestBeanGUI" testclass="BeanShellPostProcessor" testname="-&gt; 解析并打印返回值" enabled="true">
              <boolProp name="resetInterpreter">false</boolProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="script">import org.json.*;

String message = prev.getResponseDataAsString();

bsh.shared.util.info(&quot;Ide Server(active) 原始 WSS 消息: &quot; + message);

try {
    // 去掉开头的 &quot;42&quot;，解析成 JSONArray
    JSONArray jsonArray = new JSONArray(message.substring(2));
    
    // 第一个元素是事件类型
    String eventType = jsonArray.getString(0);
    
    // 第二个元素是数据对象
    JSONObject data = jsonArray.getJSONObject(1);

    bsh.shared.util.info(&quot;Ide Server(active) 事件类型: &quot; + eventType);

    // 把所有字段都记录下来（方便调试和后续使用）
    JSONArray names = data.names();
    if (names != null) {
        for (int i = 0; i &lt; names.length(); i++) {
            String key = names.getString(i);
            Object valueObj = data.get(key);
            String value = valueObj != null ? valueObj.toString() : &quot;null&quot;;
            
            bsh.shared.util.info(key + &quot;: &quot; + value);
            vars.put(key, value);
        }
    }

    // 只在存在 &quot;success&quot; 字段时才读取 success、reason、error
    if (data.has(&quot;success&quot;)) {
        boolean success = data.getBoolean(&quot;success&quot;);
        String reason = data.optString(&quot;reason&quot;, &quot;&quot;);  // 没有 reason 就给空字符串
        
        bsh.shared.util.info(&quot;Ide Server(active) success: &quot; + success);
        bsh.shared.util.info(&quot;Ide Server(active) reason: &quot; + reason);
        
        vars.put(&quot;success&quot;, String.valueOf(success));
        vars.put(&quot;reason&quot;, reason);

        if (data.has(&quot;error&quot;)) {
            String error = data.getString(&quot;error&quot;);
            bsh.shared.util.info(&quot;Ide Server(active) error: &quot; + error);
            vars.put(&quot;error&quot;, error);
        }
    }

    // 无论哪种消息，都把 eventType 存下来
    vars.put(&quot;eventType&quot;, eventType);

} catch (JSONException e) {
    bsh.shared.util.error(&quot;Ide Server(active) JSON 解析错误: &quot; + e.getMessage(), e);
}</stringProp>
