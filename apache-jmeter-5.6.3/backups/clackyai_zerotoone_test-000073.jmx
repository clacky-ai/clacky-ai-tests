            } else {
                bsh.shared.util.warn(&quot;(Ai Agent)-session 未找到有效的 session_id&quot;);
            }

        } catch (Exception e) {
            bsh.shared.util.error(&quot;(Ai Agent)-session 解析 43x 帧失败: &quot; + e.getMessage(), e);
        }
    }
} catch (Exception e) {
    bsh.shared.util.error(&quot;(Ai Agent) 整体处理异常: &quot; + e.getMessage(), e);
}</stringProp>
